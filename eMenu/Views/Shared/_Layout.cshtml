    <link href="~/css/style.css" rel="stylesheet" />
    <!-- responsive style -->
    <link href="~/css/responsive.css" rel="stylesheet" />


</head>

<body>

    <div class="hero_area">
        <div class="bg-box">
            <img src="~/images/hero-bg.jpg" alt="">
        </div>
        <!-- header section strats -->
        <partial name="~/Views/Shared/Partials/_HeaderPartial.cshtml">
            <!-- end header section -->
            <!-- slider section -->
            @RenderSection("Slider", false);
            <!-- end slider section -->
            @RenderBody();
    </div>

    <!-- offer section -->
    <!-- end offer section -->
